pipeline {
    agent {
        ecs {
            inheritFrom 'build-example-spot'
        }
    }
    stages {
      stage('PUT S3') {
          steps {
            echo 'Deploying to RDG AWS s3 bucket.'
            s3Upload(bucket: 'infinitelambda-statics-origin', includePathPattern:'**/*')
          }
      }
    }
}
